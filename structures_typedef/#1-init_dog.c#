#include "dog.h"
#include <stdio.h>

/**
 * init_dog - creates  the dog data  structure
 *
 * @d: location  of the data  structure
 * @name: name of dog
 * @age: age of dog
 * @owner: owner of the dog
 * Description: as above
 * Return: void
 */

void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d != NULL)
{
d->name = name;
d->age = age;
d->owner = owner;
}
}

void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d != NULL)
{
d->name = name;
d->age = age;
d->owner = owner;
}
}